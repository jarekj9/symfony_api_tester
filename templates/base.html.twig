<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/css/bootstrap.min.css" integrity="sha384-9aIt2nRpC12Uk9gS9baDl411NQApFmC26EwAOH8WgZl5MYYxFfc+NcPb1dKGj7Sk" crossorigin="anonymous">
        <title>{% block title %}API test!{% endblock %}</title>
        {% block stylesheets %}{% endblock %}
    </head>
    <body>
    <script src="https://code.jquery.com/jquery-3.4.1.min.js" integrity="sha256-CSXorXvZcTkaix6Yvo6HppcZGetbYMGWSFlBw8HfCJo=" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js" integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/js/bootstrap.min.js" integrity="sha384-OgVRvuATP1z7JjHLkuOU7Xw704+h835Lr+6QL9UvYjZE3Ipu6Tp75j7Bh/kR0JKI" crossorigin="anonymous"></script>
        
    
    {% if is_granted('IS_AUTHENTICATED_FULLY') %}
        <div>
        <a href="{{ path('app_logout') }}">LOGOUT</a>
        </div>

        <table class='table-info table'>
            <tr>
                <td><a href="{{ path('apirequest.sendany') }}" style=''>::Send any request</a></td>
                <td><a href="{{ path('apirequest.define') }}">::Define new request</a></td>
                <td><a href="{{ path('apirequest.send_defined') }}">::Send already defined request</a></td>
            </tr>
        </table>
    {% endif %}




    {% block body %}{% endblock %}


    {% for message in app.flashes('success') %}
    <div class="alert alert-primary" role="alert">
        {{ message|nl2br }}
    </div>
    {% endfor %}


    {% block javascripts %}{% endblock %}

   
    </body>
</html>
